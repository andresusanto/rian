{
	"name": "rian",
	"version": "0.0.2-alpha.14",
	"description": "Effective tracing for the edge and origins",
	"keywords": [
		"opentelemetry",
		"trace-context",
		"opentracing",
		"traceparent",
		"measure",
		"monitor",
		"observe",
		"tracing",
		"zipkin",
		"tracer",
		"trace"
	],
	"repository": "maraisr/rian",
	"license": "MIT",
	"author": {
		"name": "Marais Rossouw",
		"email": "me@marais.dev",
		"url": "https://marais.io"
	},
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": {
			"import": "./index.mjs",
			"require": "./index.js"
		},
		"./exporter.otel.http": {
			"import": "./exporter.otel.http.mjs",
			"require": "./exporter.otel.http.js"
		},
		"./exporter.zipkin": {
			"import": "./exporter.zipkin.mjs",
			"require": "./exporter.zipkin.js"
		},
		"./package.json": "./package.json"
	},
	"main": "./index.js",
	"module": "./index.mjs",
	"types": "index.d.ts",
	"files": [
		"*.mjs",
		"*.js",
		"*.d.ts"
	],
	"dependencies": {
		"flattie": "^1.1.0",
		"tctx": "^0.0.10"
	}
}
